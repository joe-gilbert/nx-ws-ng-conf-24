{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "movies/data-access-movies": [
        "libs/movies/data-access-movies/src/index.ts"
      ],
      "movies/feature-movie-search-control": [
        "libs/movies/feature-movie-search-control/src/index.ts"
      ],
      "movies/ui-movie-card": ["libs/movies/ui-movie-card/src/index.ts"],
      "shared/feature-not-found-page": [
        "libs/shared/feature-not-found-page/src/index.ts"
      ],
      "shared/models": ["libs/shared/models/src/index.ts"],
      "shared/ui": ["libs/shared/ui/src/index.ts"],
      "shared/ui-star-rating": ["libs/shared/ui-star-rating/src/index.ts"],
      "shared/ui-theme": ["libs/shared/ui-theme/src/index.ts"],
      "shared/util-env-token": ["libs/shared/util-env-token/src/index.ts"],
      "shared/utils": ["libs/shared/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
